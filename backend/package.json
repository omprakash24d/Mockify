{
  "name": "mockify-backend",
  "version": "1.0.0",
  "description": "Backend API for Mockify NEET question bank management",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "setup": "node scripts/setupDatabase.js",
    "seed": "node scripts/seedData.js",
    "seed-full": "npm run setup && npm run seed",
    "check": "node scripts/checkSetup.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "test": "jest"
