  "keywords": [
    "neet",
    "questions",
    "api",
    "education"
  ],
  "author": "Mockify Team",
  "license": "MIT",
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "firebase": "^12.3.0",
    "firebase-admin": "^13.5.0",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "mongoose": "^7.5.0",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "jest": "^29.6.4",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
